{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/scenarios",
      "dest": "/api/scenarios.js"
    },
    {
      "src": "/api/scenarios/(.*)",
      "dest": "/api/scenarios.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}